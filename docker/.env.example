NODE_ENV=<development || production>

APP_KEYS="toBeModified1,toBeModified2"
API_TOKEN_SALT=tobemodified
ADMIN_JWT_SECRET=tobemodified
TRANSFER_TOKEN_SALT=tobemodified
JWT_SECRET=tobemodified
ENCRYPTION_KEY=tobemodified

DATABASE_CLIENT=postgres
DATABASE_HOST=127.0.0.1
DATABASE_PORT=5432
DATABASE_NAME=strapi
DATABASE_USERNAME=<change me>
DATABASE_PASSWORD=<change me>
DATABASE_SSL=boolean

# localhost for development || domain-name/strapi for production
API_CLIENT_URL=<http://localhost:1337 || https://[domain-name]/strapi>
API_CONTAINER_URL=http://strapi:1337
API_TOKEN_DEVELOPMENT=<change me>
API_TOKEN_PRODUCTION=<change me>

MEILISEARCH_HOST=http://meilisearch:7700
MEILISEARCH_MASTER_KEY=<change me>

S3_ACCESS_KEY_ID=<change me>
S3_SECRET_ACCESS_KEY=<change me>
S3_BUCKET=marcus-dev
S3_REGION=ru-beget
S3_ENDPOINT=http://minio:9000 # production: замените на боевой endpoint Beget (например https://s3.storage.beget.tech)
S3_FORCE_PATH_STYLE=true
S3_CDN_BASE_URL=http://localhost:9000/${S3_BUCKET}
NEXT_PUBLIC_ASSET_HOST=${S3_CDN_BASE_URL}

REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_TLS=false
REDIS_CACHE_DB=0
REDIS_CACHE_PREFIX=marcus:rest-cache:
REDIS_CACHE_TTL=300
REDIS_CACHE_CONTENT_TYPES=api::category.category,api::product.product,api::product-variant.product-variant,api::page.page,api::portfolio-item.portfolio-item
