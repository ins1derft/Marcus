{
  "kind": "collectionType",
  "collectionName": "materials",
  "info": {
    "singularName": "material",
    "pluralName": "materials",
    "displayName": "Material"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "materials"
    },
    "cases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case.case",
      "mappedBy": "material"
    }
  }
}
